{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "start": "concurrently \"./vendor/bin/sail up -d && sleep 10\" \"npm run dev\"",
        "setup": "npm install && ./vendor/bin/sail up -d && sleep 10 && ./vendor/bin/sail php artisan migrate && ./vendor/bin/sail php artisan db:seed",
        "stop": "./vendor/bin/sail down"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.2.0",
        "tailwindcss": "^4.0.0",
        "typescript": "^5.8.3",
        "vite": "^6.2.4"
    },
    "dependencies": {
        "@vitejs/plugin-react": "^4.7.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    }
}
